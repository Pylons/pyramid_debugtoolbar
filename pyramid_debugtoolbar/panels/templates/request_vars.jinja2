<h4>Cookie Variables</h4>
{% if cookies %}
	<table>
		<colgroup>
			<col style="width:20%"/>
			<col/>
		</colgroup>
		<thead>
			<tr>
				<th>Variable</th>
				<th>Value</th>
			</tr>
		</thead>
		<tbody>
			{% for key, value in cookies %}
				<tr class="{{ loop.cycle('pDebugOdd', 'pDebugEven') }}">
					<td>{{ key|escape }}</td>
					<td>{{ value|escape }}</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% else %}
	<p>No cookie data</p>
{% endif %}

