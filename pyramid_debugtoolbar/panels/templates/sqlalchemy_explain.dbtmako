<div class="modal-header">
<button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
<h3 id="ExplainModalLabel">EXPLAIN</h3>
</div>
<div class="modal-body">

<div class="pDebugPanelTitle">
	<h3>SQL Explained</h3>
</div>
<div class="pDebugPanelContent">
	<div class="scroll">
		<dl>
			<dt>Executed SQL</dt>
			<dd>${sql|n}</dd>
			<dt>Time</dt>
			<dd>${'%.2f' % (duration)} ms</dd>
		</dl>
		<table class="djSqlExplain table table-striped"
>
			<thead>
				<tr>
					% for h in headers:
						<th>${h.upper()}</th>
					% endfor
				</tr>
			</thead>
			<tbody>
				% for i, row in enumerate(result):
				    <tr class="${i%2 and 'pDebugEven' or 'pDebugOdd'}">
						% for column in row:
						<td>${str(column).replace(' ', '&nbsp;')|n}</td>
						% endfor
					</tr>
				% endfor
			</tbody>
		</table>
	</div>
</div>

</div>
<div class="modal-footer">
<button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
</div>
